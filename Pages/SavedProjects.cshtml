@page
@model SavedProjectsModel
@{
    ViewData["Title"] = "Saved Projects";
}

<h1>Saved Projects</h1>
<table class="table">
    <thead>
        <tr>
            <th>Title</th>
            <th>Description</th>
            <th>Creator</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var project in Model.SavedProjects)
        {
            <tr>
                <td>@project.Title</td>
                <td>@project.Description</td>
                <td>@project.Creator</td>
            </tr>
        }
    </tbody>
</table>
